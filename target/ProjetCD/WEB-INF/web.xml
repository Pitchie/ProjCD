<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>CreationCD</servlet-name>
		<servlet-class>com.ben.cd.servlets.CreationCD</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GestionCD</servlet-name>
		<servlet-class>com.ben.cd.servlets.GestionCD</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AcceuilCD</servlet-name>
		<servlet-class>com.ben.cd.servlets.AcceuilCD</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>CreationCD</servlet-name>
		<url-pattern>/creationCD</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GestionCD</servlet-name>
		<url-pattern>/gestionCD</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AcceuilCD</servlet-name>
		<url-pattern>/acceuilCD</url-pattern>
	</servlet-mapping>
</web-app>